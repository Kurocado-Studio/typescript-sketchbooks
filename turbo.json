{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", "storybook-static/**"],
      "dependsOn": ["^internal:build", "^build"]
    },
    "internal:build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^internal:build"],
      "cache": false
    },
    "setup": {
      "dependsOn": ["^internal:build", "^setup", "^build"],
      "cache": false
    },
    "prettier:fix": {
      "dependsOn": ["^setup"],
      "cache": false
    },
    "prettier:check": {
      "dependsOn": ["^setup"],
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^setup"],
      "cache": false
    },
    "test": {
      "dependsOn": ["^setup"],
      "cache": false
    },
    "lint": {
      "dependsOn": ["^setup"],
      "cache": false
    },
    "lint:fix": {
      "dependsOn": ["^setup"],
      "cache": false
    },
    "ui:local:lib": {
      "dependsOn": ["^setup"],
      "cache": false
    },
    "ui:local:sync": {
      "dependsOn": ["^setup"],
      "cache": false
    },
    "ui:canary:lib": {
      "dependsOn": ["^setup"],
      "cache": false
    },
    "ui:published:lib": {
      "dependsOn": ["^setup"],
      "cache": false
    },
    "dev": {
      "dependsOn": ["^internal:build", "^setup"],
      "cache": false,
      "persistent": true
    },
    "dev:storybook": {
      "dependsOn": ["^internal:build", "^setup", "^build"],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "preview-storybook": {
      "dependsOn": ["^build"],
      "cache": false
    }
  }
}
