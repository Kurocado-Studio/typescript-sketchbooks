{
  "name": "@kurocado-studio/daily-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preview": "vite preview",
    "build": "storybook build",
    "clean": "rm -rf .turbo node_modules",
    "setup": "pnpm install",
    "dev:storybook": "storybook dev -p 6007 --no-open",
    "lint": "eslint ./stories/*.stories.ts --max-warnings 0 --no-warn-ignored",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --check . --write",
    "preview-storybook": "serve storybook-static",
    "ui:local:lib": "pnpm add @kurocado-studio/ui-research-and-development@file:../../../design-system/packages/ui",
    "ui:canary:lib": "pnpm add @kurocado-studio/ui-research-and-development@canary",
    "ui:published:lib": "pnpm add @kurocado-studio/ui-research-and-development@latest",
    "ui:local:sync": "cd ../../../design-system/packages/ui && pnpm run build && cd - && pnpm install @kurocado-studio/ui-research-and-development@file:../../../design-system/packages/ui",
    "storybook": "storybook dev -p 6008",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@kurocado-studio/ui-research-and-development": "1.0.0-develop.7",
    "@kurocado-studio/ui-vue-research-and-development": "1.0.0-develop.4",
    "@internal/ui": "workspace:*",
    "vue": "^3.5.15"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@internal/config": "workspace:*",
    "@storybook/addon-actions": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/addon-themes": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/experimental-addon-test": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/theming": "^8.6.14",
    "@storybook/vue3": "^8.6.14",
    "@storybook/vue3-vite": "^8.6.14",
    "@tailwindcss/vite": "^4.1.11",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.23",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/browser": "^3.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.27.0",
    "playwright": "^1.52.0",
    "postcss": "^8.4.31",
    "semantic-release": "^24.2.5",
    "semantic-release-monorepo": "^8.0.2",
    "serve": "^14.2.1",
    "storybook": "^8.6.14",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.8.3",
    "vite": "^5.2.8",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.1.10"
  }
}
